{
  "main": "dist/src/server.js",
  "scripts": {
    "postinstall": "npm run build src",
    "run-script": "node -r phoenix-api-lib/envloader",
    "build": "tsc --build --verbose",
    "build-serve": "npm run build src && npm run serve",
    "build-test": "npm run build test && npm run test",
    "serve": "npm run run-script dist/src/server.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "test": "cross-env NODE_ENV=test npx mocha --require phoenix-api-lib/envloader --require ts-node/register 'dist/test/**/*.test.js' --exit",
    "make": "node tools/make.js"
  },
  "dependencies": {
    "bcrypt": "^3.0.6",
    "bluebird": "^3.7.1",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.0.0",
    "express-session": "^1.17.0",
    "helmet": "^3.21.2",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.20.2",
    "morgan": "^1.9.1",
    "mysql": "^2.17.1",
    "mysql2": "^2.0.0",
    "phoenix-api-lib": "file:../lib",
    "redis": "^2.8.0",
    "underscore": "^1.9.1",
    "validator": "^11.1.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/chai": "^4.2.5",
    "@types/express": "^4.17.1",
    "@types/express-rate-limit": "^3.3.3",
    "@types/express-session": "^1.15.14",
    "@types/helmet": "^0.0.44",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/mocha": "^5.2.7",
    "@types/morgan": "^1.7.37",
    "@types/node": "^12.12.7",
    "@types/redis": "^2.8.14",
    "@types/underscore": "^1.9.3",
    "@types/winston": "^2.4.4",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^6.2.2",
    "nodemon": "^1.19.4",
    "ts-node": "^8.5.2",
    "typescript": "^3.7.2"
  }
}
